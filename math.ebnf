expression = identifier "(" (expression ("," expression)*)? ")" "=>" sum_expression | identifier "=" sum_expression | sum_expression;
sum_expression = factor_expression ("+" | "-") factor_expression | factor_expression;
factor_expression = sign_expression ("*" | "/" | "%") sign_exprsesion | sign_expression;
sign_expression = ("+" | "-") exponentiation_expression | exponentiation_expression;
exponentiation_expression = primary_expression "^" primary_expression | primary_expression;
primary_expression = identifier "(" (expression ("," expression)*)? ")" | "(" expression ")" | constant | identifier;

constant = /[+-]?[0-9]+(\.[0-9]+)?/;
identifier = /[_a-zA-Z][_a-zA-Z0-9]*/;